<h1>Listing teams</h1>

<table>
  <tr>
    <th>Division</th>
    <th>Name</th>
    <th>Short</th>
    <th>Mascot</th>
    <th>Seed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.division %></td>
    <td><%= team.name %></td>
    <td><%= team.short_name %></td>
    <td><%= team.mascot %></td>
    <td><%= team.seed %></td>
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Destroy', team, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team', new_team_path %>
